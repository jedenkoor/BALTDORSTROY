  section.promo.margin-section
    img.promo__img(src="images/promo.jpg" alt="Промо")
    .promo__content 
      span.subtitle Наш узкий профиль
      h2.promo__title.h2title О спорт, ты — лёд, вода и БАЛТПРОМХОЛОД
